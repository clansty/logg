{
  "name": "@guiiai/logg",
  "version": "1.0.0",
  "description": "Yet another simple, nearly zero dependency, structural log compliance logger implementation.",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
