    "build": "unbuild",
    "package:publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "packageManager": "pnpm@9.6.0",
  "dependencies": {
    "colorette": "^2.0.20"
  },
  "devDependencies": {
    "unbuild": "^2.0.0"
  }
}
